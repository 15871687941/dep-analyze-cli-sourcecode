{
  "name": "dep-analyze-cli",
  "version": "1.1.0",
  "description": "display dependency information of the package and run a server to graphical display",
  "main": "./index.js",
  "scripts": {
    "start": "ts-node ./index.ts analyze",
    "build": "tsc && cp ./package.json ../dep-analyze-cli/package.json && cp -r ./vue ../dep-analyze-cli/vue && cp ./README.md ../dep-analyze-cli/README.md",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{ts, json}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "bin": {
    "dep-analyze-cli": "bin/bin-linux.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "semver": "^7.5.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.9",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vitest/coverage-istanbul": "^0.34.3",
    "@vitest/coverage-v8": "^0.34.3",
    "eslint": "^8.47.0",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "lint-staged": "^14.0.1",
    "open": "^9.1.0",
    "prettier": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vitest": "^0.34.3"
  },
  "keywords": ["package", "analyze", "dependencies", "graphical display"]
}
